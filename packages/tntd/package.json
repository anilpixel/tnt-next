    "@rushstack/eslint-config": "~2.5.1",
    "@rushstack/heft": "~0.44.2",
    "@rushstack/heft-storybook-plugin": "~0.1.91",
    "@tntd/heft-rig": "workspace:*",
    "@tntd/storybook-react-storykit": "workspace:*",
