{
  "name": "@tnt-next/components",
  "version": "0.1.1",
  "scripts": {
    "build": "npm run build:es && npm run build:lib",
    "build:es": "rimraf es && tsc -p tsconfig.build.json --outDir es && copyfiles -u 1 src/**/*.less es",
    "build:lib": "rimraf lib && tsc -p tsconfig.build.json -t ES3 -m commonjs --outDir lib && copyfiles -u 1 src/**/*.less lib/"
  },
  "dependencies": {
    "@icon-park/react": "^1.3.5",
    "@tnt-next/illustrations": "^0.1.0",
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.176",
    "@types/react": "^16.14.20",
    "@types/react-dom": "^16.9.14",
    "@types/styled-components": "^5.1.15",
    "antd": "^3.26.19",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "antd": "^3.26.19",
    "react": "16 || 17",
    "react-dom": "16 || 17"
  },
  "files": [
    "es",
    "lib"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts"
}
